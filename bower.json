{
	"name": "Trigger",
	"version": "0.5.0",
	"homepage": "https://github.com/johndevlin/Trigger",
	"authors": [
		"johndevlin <j@johndevlin.me>"
	],
	"description": "Trigger is a lightweight front-end framework sitting on top of Grunt.js to automatically combine and minify all JavaScript, optimise images, and run SASS.",
	"main": "main.scss",
		"keywords": [
		"SASS",
		"Grunt",
		"HTML"
	],
	"license": "MIT",
	"private": true,
	"ignore": [
		"**/.*",
		"node_modules",
		"bower_components",
		"test",
		"tests"
	],
	"dependencies": {
		"bourbon": "4.0",
		"neat": "1.6",
		"jquery-placeholder": "2.0.8",
		"jquery.debouncedresize": "~1"
	}
}